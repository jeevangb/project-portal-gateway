# GraphQL schema example
#
# https://gqlgen.com/getting-started/


type Query {
  healthCheck: HealthStatus
}

type HealthStatus{
  status: Int!
  message: String! 
}

input NewUser {
  name: String!
  email: String!
  password: String!
}

type UserResponse {
  message: String!
  token: String 
}

type Mutation {
  signUp(input: NewUser!): UserResponse!
  login(email: String!, password: String!): UserResponse!
}
